#include <stdio.h>
int main() {
    int a[100],i,n;
    printf("enter the size:");
    scanf("%d", &n);
    printf("enter the elements:");
    for(i=0;i<n;i++){
        scanf("%d",&a[i]);
    }
    printf("the given elements are:");
    for(i=0;i<n;i++){
        printf("\n%d",a[i]);
    }
    return 0;
}
